{
  "name": "as-milano/yii2-assets",
  "description": "Useful assets (icons, js and css components) for Yii2 Framework.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "assets",
    "icons",
    "font-awesome",
    "whhg",
    "prettyLoader",
    "prettyPhoto",
    "photobox"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "NecroMan",
      "email": "nm666@bk.ru"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "~2.0",
    "bower-asset/jquery-migrate": "~1.2",
    "bower-asset/font-awesome": "dev-master",
    "bower-asset/jquery.lazyload": "~1.9",
    "bower-asset/jcarousel": "dev-master",
    "bower-asset/superfish": "~1.7",
    "bower-asset/photobox": "~1.9",
    "bower-asset/remarkable-bootstrap-notify": "dev-master",
    "bower-asset/animate.css": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "milano\\assets\\": "assets",
      "milano\\widgets\\": "widgets",
      "milano\\helpers\\": "helpers"
    }
  }
}
